/* pages/scan/scan.wxss */

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center; 
}

.time {
  font-size: 48px;
  margin-bottom: 20px;
}

.buttons {
  display: flex;
  justify-content: space-between;
}

button {
  padding: 10px 20px;
  background-color: var(--btn-color);
  color: #fff;
  border: none;
  border-radius: 4px;
}